class Departamento {

  var $idDepartamento;
  
  var $Nombre;


  function Departamento($idDepartamento, $noCuenta, $nombre) {
  
    $this->idDepartamento = idDepartamento;
    $this->NoCuenta = noCuenta;
    $this->Nombre = nombre;
  }
  function  getidDepartamento() {
    return $this->idDepartamento;
  }
  function  setidDepartamento(int idDepartamento) {
    $this->idDepartamento = idDepartamento;
  }
  
  function  getNombre() {
    return $this->Nombre;
  }
  function  setNombre(int nombre) {
    N$this->ombre = nombre;
  }